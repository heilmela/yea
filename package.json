{
  "name": "YEA",
  "version": "0.0.1",
  "description": "YEt Another express template.",
  "main": "server.js",
  "scripts": {
    "lint": "eslint --fix src/ && echo 'Lint completed.'",
    "start": "NODE_ENV=production npm run build && npm run start-app",
    "start-app": "NODE_ENV=production node ./build/server.js",
    "test": "NODE_ENV=test nyc mocha",
    "build": "NODE_ENV=production rimraf build/ && babel ./src/ --out-dir build/ --ignore ./node_modules,./docker",
    "dev": "NODE_ENV=development nodemon --exec babel-node --presets @babel/env ./src/server.js",
    "test-unit": "NODE_ENV=test nyc mocha './test/unit/**/*.js' './test/config.js'",
    "test-integration": "NODE_ENV=test nyc mocha './test/integration/**/*.js' './test/config.js'",
    "debug": "NODE_ENV=development nyc mocha --require @babel/core --require @babel/register --inspect"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:heilmela/yea.git"
  },
  "keywords": [
    "YEA",
    "express",
    "express.js",
    "template",
    "quick",
    "start",
    "api",
    "REST"
  ],
  "author": "laurin@laph.xyz",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@hapi/joi": "^17.1.1",
    "awilix": "^4.2.7",
    "awilix-express": "^3.0.0",
    "bcrypt": "^3.0.8",
    "celebrate": "^13.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-enrouten": "^1.3.0",
    "express-session": "^1.17.1",
    "generate-safe-id": "^1.0.3",
    "json-stringify-pretty-compact": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.11",
    "passport": "^0.4.1",
    "promise-retry": "^2.0.1",
    "uuid": "^3.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unused-imports": "^0.1.3",
    "faker": "^4.1.0",
    "mocha": "^7.2.0",
    "mocha-logger": "^1.0.7",
    "mocha-steps": "^1.3.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.1.2",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2"
  }
}
